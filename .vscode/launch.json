    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug UncertainBird",
            "type": "debugpy",
            "request": "launch",
            "program": "./birdset/train.py --config-path '../projects/uncertainbird/configs' --config-dir 'configs' 'logger=wandb_UncertainBird'",
            // "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}",
            "justMyCode": false
        },
        {
            "name": "Debug Dump Predictions",
            "type": "debugpy",
            "request": "launch",
            "program": "./uncertainbird/scripts/dump_predictions.py",
            "args": [
                "--model",
                "birdmae",
                "--datasets",
                "NBP",
                "--gpu",
                "0",
                "--output-dir",
                "./logs/new"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}",
            "justMyCode": false
        }
    ]
}